Description
    Writes out values of fields from cells nearest to specified locations.

\*---------------------------------------------------------------------------*/

#includeEtc "caseDicts/postProcessing/probes/probes.cfg"

region solid;

fields (T);

writeControl    timeStep;
writeInterval   50;

probeLocations
(
    (0.01 0.05 0.09)
    (0.01 0.05 0.07)
    (0.01 0.05 0.05)
    (0.01 0.05 0.03)
    (0.01 0.05 0.01)
);

