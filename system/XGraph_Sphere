FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      singleGraph;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Profil de température le long de X dans la sphère
// Ligne passant par le centre (Y=Z=Center)

type            sets;
libs            (sampling);

region          sphere;

writeControl    writeTime;

interpolationScheme cellPoint;

setFormat       csv;

sets
(
    line
    {
        type    lineUniform;
        axis    x;
        start   (0.02010 0.05000 0.05000);
        end     (0.07990 0.05000 0.05000);
        nPoints 50;
    }
);

fields          (T);

// ************************************************************************* //
