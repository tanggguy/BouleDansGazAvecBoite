FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "50/gas";
    object      Qdot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    gas
    {
        type            calculated;
        value           uniform 0;
    }
    gas_to_box_xmin
    {
        type            calculated;
        value           uniform 0;
    }
    gas_to_box_xmax
    {
        type            calculated;
        value           uniform 0;
    }
    gas_to_sphere
    {
        type            calculated;
        value           uniform 0;
    }
}


