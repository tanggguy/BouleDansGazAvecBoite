FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/gas";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    // Air - Gaz parfait

    specie
    {
        molWeight       28.97;  // [g/mol] Masse molaire de l'air
    }
    thermodynamics
    {
        Cp              1005;   // [J/(kg*K)] Capacité thermique à pression constante
        Hf              0;
    }
    transport
    {
        mu              1.8e-5;   // [Pa*s] Viscosité dynamique à ~300K
        Pr              0.71;     // Nombre de Prandtl de l'air
    }
}

