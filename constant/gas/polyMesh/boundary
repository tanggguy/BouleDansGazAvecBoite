FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/gas/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

4
(
    gas
    {
        type            wall;
        inGroups        1(wall);
        nFaces          1592;
        startFace       19728;
    }
    gas_to_box_xmin
    {
        type            mappedWall;
        inGroups        2(wall mappedPatch);
        nFaces          400;
        startFace       21320;
        sampleMode      nearestPatchFace;
        sampleRegion    box_xmin;
        samplePatch     box_xmin_to_gas;
    }
    gas_to_box_xmax
    {
        type            mappedWall;
        inGroups        2(wall mappedPatch);
        nFaces          400;
        startFace       21720;
        sampleMode      nearestPatchFace;
        sampleRegion    box_xmax;
        samplePatch     box_xmax_to_gas;
    }
    gas_to_sphere
    {
        type            mappedWall;
        inGroups        2(wall mappedPatch);
        nFaces          672;
        startFace       22120;
        sampleMode      nearestPatchFace;
        sampleRegion    sphere;
        samplePatch     sphere_to_gas;
    }
)

