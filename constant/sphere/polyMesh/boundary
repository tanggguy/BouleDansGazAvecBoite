FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/sphere/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

1
(
    sphere_to_gas
    {
        type            mappedWall;
        inGroups        2(wall mappedPatch);
        nFaces          672;
        startFace       6432;
        sampleMode      nearestPatchFace;
        sampleRegion    gas;
        samplePatch     gas_to_sphere;
    }
)

