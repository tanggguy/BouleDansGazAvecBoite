FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/sphere";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    // Mousse de polyuréthane (PU) - Valeurs typiques

    specie
    {
        molWeight       100;    // [g/mol] Valeur approximative (peu d'impact pour solides)
    }
    equationOfState
    {
        rho             50;     // [kg/m^3] Densité mousse PU rigide (30-50 kg/m³ typique)
    }
    transport
    {
        kappa           0.025;  // [W/(m*K)] Conductivité thermique (très isolant)
    }
    thermodynamics
    {
        Hf              0;
        Cp              1500;   // [J/(kg*K)] Capacité thermique
    }
}

